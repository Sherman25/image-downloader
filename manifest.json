{
    "name": "Bulk Image Downloader",
    "description": "Download every image on a web page",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "permissions": [],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "manifest_version": 2
}
